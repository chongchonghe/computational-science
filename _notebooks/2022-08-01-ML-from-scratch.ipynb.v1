   "source": [
    "def predict(w, b, X):\n",
    "    A = sigmoid(np.dot(w.T, X) + b)[0]\n",
    "    # Iterating through A and predict an 1 if the value of A is greater than 0.5 and a 0 otherwise\n",
    "    return np.array(A > 0.5, dtype=int)"
   ]
  },
